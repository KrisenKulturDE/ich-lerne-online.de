	"scripts": {
		"dev": "npm run dev:legacy && npm run dev:modern",
		"dev:modern": "webpack --config webpack.config.js --mode development --clear false",
		"dev:legacy": "webpack --config webpack.config.js --mode development --browser_env legacy --clear false",
		"watch": "webpack --watch --config webpack.config.js --mode development --watch-poll",
		"build:legacy": "webpack --config webpack.config.js --mode production --browser_env legacy",
		"build:modern": "webpack --config webpack.config.js --mode production --browser_env modern --clear false",
		"build": "npm run build:legacy && npm run build:modern",
		"deployment": "node ./bin/deployment.js"
	},
